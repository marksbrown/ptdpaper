@article{Schaart2009,
abstract = {Silicon photomultipliers (SiPMs) are of great interest to positron emission tomography (PET), as they enable new detector geometries, for e.g., depth-of-interaction (DOI) determination, are MR compatible, and offer faster response and higher gain than other solid-state photosensors such as avalanche photodiodes. Here we present a novel detector design with DOI correction, in which a position-sensitive SiPM array is used to read out a monolithic scintillator. Initial characterization of a prototype detector consisting of a 4 x 4 SiPM array coupled to either the front or back surface of a 13.2 mm x 13.2 mm x 10 mm LYSO:Ce(3+) crystal shows that front-side readout results in significantly better performance than conventional back-side readout. Spatial resolutions <1.6 mm full-width-at-half-maximum (FWHM) were measured at the detector centre in response to an approximately 0.54 mm FWHM diameter test beam. Hardly any resolution losses were observed at angles of incidence up to 45 degrees , demonstrating excellent DOI correction. About 14\% FWHM energy resolution was obtained. The timing resolution, measured in coincidence with a BaF(2) detector, equals 960 ps FWHM.},
author = {Schaart, Dennis R and van Dam, Herman T and Seifert, Stefan and Vinke, Ruud and Dendooven, Peter and L\"{o}hner, Herbert and Beekman, Freek J},
doi = {10.1088/0031-9155/54/11/015},
file = {:home/mbrown/Documents/Mendeley/Schaart et al. - 2009 - A novel, SiPM-array-based, monolithic scintillator detector for PET.pdf:pdf},
issn = {0031-9155},
journal = {Physics in medicine and biology},
keywords = {Algorithms,Models, Theoretical,Positron-Emission Tomography,Positron-Emission Tomography: methods,Scintillation Counting,Scintillation Counting: instrumentation,Time Factors},
month = jun,
number = {11},
pages = {3501--12},
pmid = {19443953},
title = {{A novel, SiPM-array-based, monolithic scintillator detector for PET.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19443953},
volume = {54},
year = {2009}
}
@article{Yu2010,
abstract = {Establishing the fundamental limit of nanophotonic light-trapping schemes is of paramount importance and is becoming increasingly urgent for current solar cell research. The standard theory of light trapping demonstrated that absorption enhancement in a medium cannot exceed a factor of 4n(2)/sin(2)$\theta$, where n is the refractive index of the active layer, and $\theta$ is the angle of the emission cone in the medium surrounding the cell. This theory, however, is not applicable in the nanophotonic regime. Here we develop a statistical temporal coupled-mode theory of light trapping based on a rigorous electromagnetic approach. Our theory reveals that the conventional limit can be substantially surpassed when optical modes exhibit deep-subwavelength-scale field confinement, opening new avenues for highly efficient next-generation solar cells.},
author = {Yu, Zongfu and Raman, Aaswath and Fan, Shanhui},
doi = {10.1073/pnas.1008296107},
file = {:home/mbrown/Documents/Mendeley/Yu, Raman, Fan - 2010 - Fundamental limit of nanophotonic light trapping in solar cells(2).pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Absorption,Engineering,Engineering: methods,Light,Models, Theoretical,Nanostructures,Nanostructures: chemistry,Solar Energy,Solar Energy: statistics \& numerical data},
month = oct,
number = {41},
pages = {17491--6},
pmid = {20876131},
title = {{Fundamental limit of nanophotonic light trapping in solar cells.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2955111\&tool=pmcentrez\&rendertype=abstract},
volume = {107},
year = {2010}
}
@article{Shao2007,
abstract = {The coincidence timing resolution is a critical parameter which to a large extent determines the system performance of positron emission tomography (PET). This is particularly true for time-of-flight (TOF) PET that requires an excellent coincidence timing resolution (<<1 ns) in order to significantly improve the image quality. The intrinsic timing resolution is conventionally calculated with a single-exponential timing model that includes two parameters of a scintillator detector: scintillation decay time and total photoelectron yield from the photon-electron conversion. However, this calculation has led to significant errors when the coincidence timing resolution reaches 1 ns or less. In this paper, a bi-exponential timing model is derived and evaluated. The new timing model includes an additional parameter of a scintillator detector: scintillation rise time. The effect of rise time on the timing resolution has been investigated analytically, and the results reveal that the rise time can significantly change the timing resolution of fast scintillators that have short decay time constants. Compared with measured data, the calculations have shown that the new timing model significantly improves the accuracy in the calculation of timing resolutions.},
author = {Shao, Yiping},
doi = {10.1088/0031-9155/52/4/016},
file = {:home/mbrown/Documents/Mendeley/Shao - 2007 - A new timing model for calculating the intrinsic timing resolution of a scintillator detector.pdf:pdf},
issn = {0031-9155},
journal = {Physics in medicine and biology},
keywords = {Algorithms,Biological,Computer-Assisted,Electrons,Image Interpretation,Models,Photons,Positron-Emission Tomography,Positron-Emission Tomography: instrumentation,Positron-Emission Tomography: methods,Radiation Dosage,Reproducibility of Results,Scintillation Counting,Scintillation Counting: instrumentation,Scintillation Counting: methods,Sensitivity and Specificity,Time Factors},
month = feb,
number = {4},
pages = {1103--17},
pmid = {17264373},
title = {{A new timing model for calculating the intrinsic timing resolution of a scintillator detector.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17264373},
volume = {52},
year = {2007}
}
